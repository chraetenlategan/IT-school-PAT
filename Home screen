{$R *.dfm}

procedure TForm1.img_loginClick(Sender: TObject);
begin
// this butten takes you to the login screen

//shows the login form
form3.show;
//hides the current form
form1.Hide;
end;

procedure TForm1.img_helpClick(Sender: TObject);
begin
// this butten takes yo to Help form

//shows the help screen
form4.Show;
//hides the current form
form1.Hide;
end;

procedure TForm1.img_conctactClick(Sender: TObject);
begin
//this butten takes you to Contact Support form

//hides my current form
form1.Hide;
//This is my info form
form2.Show;
end;

procedure TForm1.img_exitClick(Sender: TObject);
begin
// the program can not be rated becasue it has not been used
Application.Terminate;

end;

end.


